#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[])
 {
 	float peso1, peso2, nota;

 	printf(" Digite a primeira nota: \n");
 	scanf("%f", &peso1);
 	
 	printf("Digite a segunda nota: \n");
 	scanf("%f", &peso2);
 	
 nota = ((peso1*4) + (peso2*6))/10;
 	
 	printf("\nA nota final e de: %.2f", nota);
 	
	return 0;
}
