#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) 
{
	
	int posicao;
	printf("Digite sua posicao: ");
	scanf("%d", &posicao);
	
	if(1<=posicao && posicao<=4)
	{ printf("Voces estao na fase de grupos da copa libertadores");
	}
	else if(posicao==5||posicao==6)
	{ printf("Voces estao nas qualificatorias para copa libertadores");
	}
	else if(7<=posicao && posicao<=12)
	{
	printf("Voces estao na sul americana");
  }
  else if(13<=posicao && posicao<=16)
  {
  	printf("Voces estao na 1 divisao");
  }
  else if(17<=posicao && posicao<=20)
  {
  	printf("Voces estao na 2 divisao");
  }
  else if(posicao<1 && 20<posicao)
  {
  	printf("classificacao invalida");
  }
  	
	return 0;
}
