#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) 
{
	int num1;
	printf("Digite um numero inteiro");
	scanf("%d", &num1);
	
	int num2;
	printf("Digite outro numero inteiro");
	scanf("%d", &num2);
	
	int num3;
	printf("Digite mais numero inteiro");
	scanf("%d", &num3);
	
	int num4;
	printf("Digite mais outro numero inteiro:");
	scanf("%d", &num4);
	
	int total;
	total = num1 + num2 + num3 + num4;
	
	printf(" O valor total da soma eh %d", total);
	
	return 0;
}
